{
  "name": "polyguard-nuxt",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "typecheck:ci": "nuxt typecheck --no-build",
    "format": "prettier --check .",
    "format:fix": "prettier --write --list-different .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf .nuxt dist .output .netlify node_modules && npm install",
    "clean:deep": "rimraf package-lock.json && npm run clean",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts",
    "codegen:diff-check": "npm run codegen && git diff --exit-code server/codegen/",
    "pretypecheck": "npm run codegen",
    "prepare": "husky",
    "postinstall": "nuxt prepare",
    "init-db": "tsx --tsconfig ./server/tsconfig.json ./server/db/init-turso-db.ts"
  },
  "dependencies": {
    "@dotlottie/player-component": "^2.7.12",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@libsql/client": "^0.14.0",
    "@lottiefiles/lottie-interactivity": "^1.6.2",
    "@nuxt/image": "^1.9.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/sitemap": "^7.2.5",
    "@pinia/nuxt": "^0.10.1",
    "@sendgrid/mail": "^8.1.4",
    "@sentry/nuxt": "^9.1.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/nuxt": "^12.7.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "h3-zod": "^0.5.3",
    "intl-tel-input": "^25.3.0",
    "jose": "^5.10.0",
    "nuxt": "^3.15.4",
    "pinia": "^3.0.1",
    "swiper": "^11.2.4",
    "tailwind-scrollbar-hide": "^2.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "vue3-carousel-nuxt": "^1.1.5",
    "vue3-social-sharing": "^1.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.6.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@nuxt/eslint": "^1.1.0",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcrypt": "^5.0.2",
    "@types/intl-tel-input": "^18.1.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "sass": "^1.85.0",
    "tsx": "^4.19.1",
    "typescript": "5.7.3",
    "vue-tsc": "2.2.0"
  },
  "overrides": {
    "@vercel/nft": "^0.27.4"
  }
}
